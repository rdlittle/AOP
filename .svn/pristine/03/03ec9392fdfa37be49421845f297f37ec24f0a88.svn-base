package com.webfront.view;

import java.util.Map;
import javax.faces.bean.ManagedBean;
import javax.faces.bean.SessionScoped;
import org.primefaces.context.RequestContext;
import org.primefaces.event.SelectEvent;

/**
 *
 * @author rlittle
 */
@ManagedBean
@SessionScoped
public class AffiliateOrderView {
  
    public String viewOrder(String orderId) {
        RequestContext.getCurrentInstance().openDialog("newxhtml");
        return orderId;
    }
    
    public void handleReturn(SelectEvent evt) {
        String str=evt.toString();
        Map map=RequestContext.getCurrentInstance().getAttributes();
        str=map.toString();
        str="";
    }
}
